find . -name 1-strncat.c -o -name 100-atoi.c -o -name 2-strchr.c -o -name 2-strlen.c -o -name 2-strncpy.c -o -name 3-islower.c -o -name 3-puts.c -o -name 3-strcmp.c -o -name 3-strspn.c -o -name 4-isalpha.c -o -name 4-strpbrk.c -o -name 5-strstr.c -o -name 6-abs.c -o -name 9-strcpy.c -o -name _putchar.c | xargs -i gcc -Wall -Werror -Wextra -pedantic -c `basename {}`