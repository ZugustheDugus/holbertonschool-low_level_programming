#include "main.h"

/**
 * main - The main function
 * @argc: the number of arguments made
 * @argv: the array of arguments made (numbered 0+)
 * Return: returns success
 */

int main(int argc, char* argv[])
{
int i;
for (i = 0; i < argc ; i++)
{
_putchar(*argv[i]);
_putchar('\n');
}
return (0);  
}
