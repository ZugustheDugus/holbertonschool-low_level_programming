#include <stdio.h>
#include <stdlib.h>
#include "3-calc.h"

/**
 * op_add - Operating function to add two integers
 * @a: integer 1
 * @b: integer 2
 * Return: Always 0.
 */

int op_add(int a, int b)
{
int res, argc;
char *argv[];

 if (argc != 3)
   {
     printf("Error\n");
     return (1);
   }
 
a = atoi(argv[1]);
b = atoi(argv[2]);

res = (a + b);
return (0);
}
/**
 * op_sub - Func to subtract two integers
 * @a: integer 1
 * @b: integer 2
 * Return: Always 0.
 */

int op_sub(int a, int b)
{

int res, argc;
char *argv[];

 if (argc != 3)
   {
     printf("Error\n");
     return (1);
   }
 
a = atoi(argv[1]);
b = atoi(argv[2]);

res = (a - b);
return (0);
}
/**
 * op_mul - multiply two integers
 * @a: int 1
 * @b: int 2
 * Return: Always 0.
 */

int op_mul(int a, int b)
{
int res, argc;
char *argv[];

 if (argc != 3)
   {
     printf("Error\n");
     return (1);
   }

a = atoi(argv[1]);
b = atoi(argv[2]);

res = (a * b);
return (0);
}

/**
 * op_div - divide two integers
 * @a: int 1
 * @b: int 2
 * Return: Always 0.
 */

int op_div(int a, int b)
{
int res, argc;
char *argv[];

 if (argc != 3)
   {
     printf("Error\n");
     return (1);
   }
 
a = atoi(argv[1]);
b = atoi(argv[2]);

res = (a / b);
return (0);
}

/**
 * op_mod - get the remainder of division of two ints
 * @a: int 1
 * @b: int 2
 * Return: Always 0.
 */

int op_mod(int a, int b)
{
int res, argc;
char *argv[];

 if (argc != 3)
   {
     printf("Error\n");
     return (1);
   }
 
a = atoi(argv[1]);
b = atoi(argv[2]);

res = (a % b);
return (0);
}
